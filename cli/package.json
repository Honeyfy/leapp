{
  "name": "leapp",
  "description": "Command Line Interface for Leapp",
  "version": "0.0.0",
  "author": "Mario Agati @agatim",
  "bin": {
    "leapp": "./bin/run"
  },
  "bugs": "https://github.com/agatim/leapp/issues",
  "dependencies": {
    "@oclif/command": "^1.8.4",
    "@oclif/config": "^1.17.1",
    "@oclif/plugin-help": "^3.2.7",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.3",
    "@types/node": "^10.17.60",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/agatim/leapp",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "leapp",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "agatim/leapp",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "prebuild": "rm -rf ./dist",
    "build": "tsc -p ./"
  },
  "types": "lib/index.d.ts"
}
